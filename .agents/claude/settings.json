{
	"includeCoAuthoredBy": true,
	"permissions": {
		"allow": [
			"Bash(gh pr view :*)",
			"Bash(gh pr diff :*)",
			"Bash(gh pr checks :*)"
		],
		"deny": [
			"Read(id_rsa)",
			"Read(id_ed25519)",
			"Read(.env)",
			"Read(.env.local)",
			"Read(.env.production)",
			"Write(.env)",
			"Write(.env.local)",
			"Write(.env.production)"
		],
		"ask": [
			"Bash(sudo:*)",
			"Bash(rm:*)",
			"Bash(rm -rf:*)",
			"Bash(git push:*)",
			"Bash(git reset:*)",
			"Bash(git rebase:*)",
			"Bash(curl:*)",
			"Bash(wget:*)",
			"Bash(psql:*)",
			"Bash(mysql:*)",
			"Bash(mongod:*)"
		]
	},
	"hooks": {
		"PostToolUse": [
			{
				"matcher": "Write",
				"hooks": []
			}
		]
	},
	"enabledPlugins": {
		"github@siraken-plugins": true,
		"common@siraken-plugins": true,
		"dev@siraken-plugins": true
	},
	"alwaysThinkingEnabled": true
}
